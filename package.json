{
  "name": "red-tetris",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:server": "tsx watch --tsconfig server/tsconfig.json server/src/index.ts",
    "dev:client": "vite build --watch",
    "build": "vite build && cp -r client/dist server/public",
