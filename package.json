    "express": "^4.21.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.28.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/express": "^5.0.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.2",
